WAIT SECONDS=2
TAB T=2
WAIT SECONDS=4
TAG POS=2 TYPE=SPAN ATTR=TXT:Like
WAIT SECONDS=5
TAB T=1
WAIT SECONDS=3
TAB CLOSEALLOTHERS
WAIT SECONDS=2
TAG POS=2 TYPE=DIV ATTR=TXT:Confirm
